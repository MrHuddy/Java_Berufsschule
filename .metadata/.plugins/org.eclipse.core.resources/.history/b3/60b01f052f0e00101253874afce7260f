package benutzerschnittstelle;
import fachkonzeptklasse.*;
public class Konsole {

	public static void main(String[] args) {
		
		Wuerfel einWuerfel = new Wuerfel();
		int count =0;
		for (int i =0; i < 1; i++) {
			einWuerfel.wirf();
			if (einWuerfel.getAugenzahl() == 6) {
				count++;
			}
		}

		System.out.println(count);
	}

}
